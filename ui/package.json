{
  "name": "ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.25.10",
    "htm": "^3.1.1",
    "preact": "^10.27.2"
  },
  "scripts": {
    "dev": "esbuild main.js --bundle --format=iife --outfile=../unotebook.js --watch",
    "build": "mkdir -p ../dist && esbuild main.js --bundle --minify --format=iife --outfile=../dist/unotebook.js --define:process.env.NODE_ENV='\"production\"' && gzip -fk ../dist/unotebook.js && ls -lh ../dist/unotebook.js.gz"
  },
  "dependencies": {
    "preact-hooks": "^0.0.0-0.0.0",
    "preact-router": "^4.1.2",
    "snarkdown": "^2.0.0"
  }
}
